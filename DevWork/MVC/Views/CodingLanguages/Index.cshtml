@model IEnumerable<Data.CodingLanguage>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<button class="btn btn-warning" role="button">@Html.ActionLink("Create New", "Create")</button>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CodingLanguageName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodingLanguageName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.CodingLanguageId }) |
                @Html.ActionLink("Details", "Details", new { id = item.CodingLanguageId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.CodingLanguageId })
            </td>
        </tr>
    }

</table>
